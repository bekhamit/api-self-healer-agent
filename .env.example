# Anthropic API Key for Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Claude Model (check https://docs.anthropic.com/en/docs/models-overview for available models)
# Common options: claude-3-5-sonnet-20241022, claude-3-opus-20240229, claude-3-sonnet-20240229
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Postman API Key
POSTMAN_API_KEY=your_postman_api_key_here

# Parallel AI API Key
PARALLEL_API_KEY=your_parallel_api_key_here

# Redis URL for vector memory (optional, defaults to redis://localhost:6379)
REDIS_URL=redis://localhost:6379

# Demo Configuration
POSTMAN_COLLECTION_ID=your_collection_id_here
POSTMAN_REQUEST_ID=your_request_id_here
